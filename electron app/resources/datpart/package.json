{
  "name": "DatPart",
  "productName": "DatPart",
  "description": "Load Dat's in your web browser.",
  "homepage": "https://github.com/HughIsaacs2/DatPart",
  "repository": {
    "type": "git",
    "url": "https://github.com/HughIsaacs2/DatPart.git"
  },
  "version": "0.0.6",
  "author": "Hugh Isaacs II <hughisaacs@acrylicstyle.com>",
  "license": "MIT",
  "main": "launch.js",
  "dependencies": {
    "chrome-native-messaging": "^0.2.0",
    "dat-node": "^3.5.4",
    "node-dat-archive": "^1.5.0"
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "ia32"
          ]
        }
      ],
      "icon": "favicon.ico"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "category": "Network"
    }
  },
  "devDependencies": {
    "electron": "1.7.11"
  }
}
